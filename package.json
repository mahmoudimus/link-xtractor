  "author": "mahmoudimus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mahmoudimus/link-xtractor.git"
  },
  "dependencies": {
    "jquery": "^3.5.0",
    "moment": "~2.24.0",
    "nan": "^2.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@types/chrome": "0.0.100",
    "@types/clipboard": "^2.0.1",
    "@types/jest": "^25.2.3",
    "@types/jquery": "^3.3.33",
    "@types/puppeteer": "^3.0.1",
    "copy-webpack-plugin": "^5.1.1",
    "clipboard": "^2.0.6",
    "clipboardy": "^2.3.0",
    "crx-hotreload": "^1.0.4",
    "execa": "^4.0.2",
    "ignore-styles": "^5.0.1",
    "jest": "^25.1.0",
    "jest-cli": "^26.1.0",
    "puppeteer": "~2.1.1",
    "rimraf": "^3.0.2 ",
    "ts-jest": "^25.2.1 ",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3",
    "webpack": "~4.42.0",
    "webpack-cli": "~3.3.11",
    "webpack-merge": "~4.2.2"
